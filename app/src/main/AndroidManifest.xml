<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sarunassarakojis.totrackerdo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".activity.dashboardactivity.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.issuesactivity.ViewIssuesActivity"
            android:parentActivityName=".activity.dashboardactivity.DashboardActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.dashboardactivity.DashboardActivity"/>
        </activity>
    </application>

</manifest>